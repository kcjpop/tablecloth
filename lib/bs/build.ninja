bs_package_flags = -bs-package-name tablecloth-bucklescript
src_root_dir = /Users/an/nw/tablecloth
bsc = /Users/an/nw/tablecloth/node_modules/bs-platform/lib/bsc.exe
bsdep = /Users/an/nw/tablecloth/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101 -warn-error +101
bsc_flags = -bs-suffix -nostdlib -I '/Users/an/nw/tablecloth/node_modules/bs-platform/lib/ocaml' -bs-g -bs-super-errors -warn-error @A -w -4-9-30-40-41-42-102 -color always
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = -I /Users/an/nw/tablecloth/node_modules/@glennsl/bs-jest/lib/ocaml
namespace = 
bsb_dir_group = 0
bsc_group_1_includes = -I __tests__
bsc_lib_includes = -I src
rule build_ast_and_module_sets
  command = ${bsc}  ${pp_flags} ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast ${in}
  description = [34mBuilding[39m [2m${out}[22m
build __tests__/tablecloth_test.mlast : build_ast_and_module_sets $src_root_dir/__tests__/tablecloth_test.ml
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build __tests__/tablecloth_test.mlast.d : build_deps __tests__/tablecloth_test.mlast
  bsb_dir_group = 1
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} ${gentypeconfig} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build __tests__/tablecloth_test.cmj |  $src_root_dir/__tests__/tablecloth_test.bs.js __tests__/tablecloth_test.cmi : build_cmj_cmi __tests__/tablecloth_test.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:__tests__
  bs_package_includes = $bs_package_includes $bs_package_dev_includes
  bsc_extra_includes = $bsc_group_1_includes
build src/tablecloth.mliast : build_ast_and_module_sets $src_root_dir/src/tablecloth.mli
build src/tablecloth.mliast.d : build_deps src/tablecloth.mliast
rule build_cmi
  command = ${bsc} ${bs_package_flags} -bs-no-builtin-ppx-mli -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} ${gentypeconfig} -o ${out} -c  ${in}
  depfile = ${in}.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build src/tablecloth.cmi : build_cmi src/tablecloth.mliast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
build src/tablecloth.mlast : build_ast_and_module_sets $src_root_dir/src/tablecloth.ml
build src/tablecloth.mlast.d : build_deps src/tablecloth.mlast
rule build_cmj_only
  command = ${bsc} ${bs_package_flags} -bs-assume-has-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include  ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} ${gentypeconfig} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build src/tablecloth.cmj |  $src_root_dir/src/tablecloth.bs.js : build_cmj_only src/tablecloth.mlast |  src/tablecloth.cmi
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src
build build.ninja : phony  ||  src/tablecloth.mlast.d src/tablecloth.mliast.d __tests__/tablecloth_test.mlast.d
